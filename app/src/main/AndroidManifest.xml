<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musiccloudplayer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/playericone"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/playericone"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CloudSelector"
                  android:label="Select Clouds and Folders"/>

        <activity android:name=".PlayList"
            android:label="Play List"/>

        <activity android:name=".Equalizer"
            android:label="Equalizer"></activity>
    </application>

</manifest>